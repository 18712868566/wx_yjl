page {
    background: #eff0f4;
    width: 100%;
    min-height: 100vh;
}

.lottPic {
    width: 100%;
    display: block;
    height: 348rpx;
}


.duihuan {
    background: #ff7f21;
    width: 100%;
    height: 110rpx;
    position: fixed;
    bottom: 0;
    text-align: center;
    line-height: 110rpx;
    color: #fff;
    font-size: 26rpx;
}


.duihuan .tit {
    color: #ffffff;
    font-size: 36rpx;
    font-weight: bold;
}


.save-address {
    padding: 50rpx 28rpx 0;
    position: relative;
}

.page-section {
    margin-top: 20rpx;
}

.weui-cells {
    width: 450rpx;
    height: 80rpx;
    background: #fff;
    border-radius: 15rpx;
}

.weui-input {
    display: block;
    line-height: 80rpx;
    height: 80rpx;
    padding: 0 15rpx;
    font-size: 26rpx;
}

.weui-cells__title {
    margin-top: 10rpx;
    color: #999999;
    font-size: 18rpx;
    padding-left: 10rpx;
}


.picker {
    color: #9a9a9a;
    font-size: 26rpx;
}


.max-weui-cells {
    width: 100%;
    height: 80rpx;
}