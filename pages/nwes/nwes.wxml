<!--pages/nwes/nwes.wxml-->
<view class="nwes-banner">
    <swiper class="swiper" indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" interval="{{interval}}" circular="{{circular}}" previous-margin="{{previousMargin}}px" next-margin="{{nextMargin}}px" duration="{{duration}}" bindchange="swiperChange">
        <block wx:for="{{imgList}}" wx:key="*this">
            <swiper-item>
                <image class='le-img {{nowIdx==index?"le-active":""}}' src='{{item}}'></image>
            </swiper-item>
        </block>
    </swiper>
</view>

<view class="section">
    <view class="news-lists">
        <view class="item-msg">
            <text class="item-title">
                20211028活动版本更新，福利送不停
            </text>
            <text class="item-min-title">
                
            </text>
            <text class="item-date">
                11/01/2021
            </text>
              
        </view>

        <view class="item-img">
            <image class="" src="/assets/news/1.png" mode="widthFix"/>
        </view>
          
          
    </view>
</view>
  