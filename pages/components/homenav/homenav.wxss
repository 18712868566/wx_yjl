.homenav {
    width: 686rpx;
    height: 226rpx;
    background: #fff;
    margin: 10rpx auto 0;
    -moz-box-shadow: 0px 0px 8rpx rgba(0, 0, 0, .2);
    -webkit-box-shadow: 0px 0px 8rpx rgba(0, 0, 0, .2);
    box-shadow: 0px 0px 8rpx rgba(0, 0, 0, .2);
    border-radius: 20rpx;
    overflow: hidden;
    padding: 30rpx 0 0;
    display: -webkit-flex;
    /* Safari */
    display: flex;
    flex-wrap: wrap;
    justify-content: space-between;
}

.icon_pic {
    display: block;
    text-align: center;
    width: 160rpx;
}

.icon_pic .imgcove {
    display: block;
    width: 52rpx;
    height: 52rpx;
    margin: 0 auto;
}

.nav-tit {
    font-size: 20rpx;
    color: #888;
}